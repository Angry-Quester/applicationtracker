<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <joined-subclass extends="edu.khai.applicationtracker.model.Application" 
                     name="edu.khai.applicationtracker.model.application.Contract" 
                     table="contract" >

                     <key column="contractId" />
                     
                     <property name="contractNumber" column="contractNumber" not-null="false"/>
    </joined-subclass>

</hibernate-mapping>